<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hbm.Events" table="events" schema="hibernate1704">
        <id name="eventId">
            <column name="EVENT_ID" sql-type="bigint(20)"/>
        </id>
        <property name="eventDate">
            <column name="EVENT_DATE" sql-type="datetime" not-null="true"/>
        </property>
        <property name="title">
            <column name="title" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>